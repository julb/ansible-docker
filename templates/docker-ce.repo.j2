[docker-ce-stable]
name=Docker CE Stable - $basearch
baseurl={{ docker_ce_centos_yum_repository_url }}/$releasever/$basearch/stable
enabled=1
gpgcheck=1
gpgkey={{ docker_ce_centos_yum_repository_url }}/gpg

[docker-ce-stable-debuginfo]
name=Docker CE Stable - Debuginfo $basearch
baseurl={{ docker_ce_centos_yum_repository_url }}/$releasever/debug-$basearch/stable
enabled=0
gpgcheck=1
gpgkey={{ docker_ce_centos_yum_repository_url }}/gpg

[docker-ce-stable-source]
name=Docker CE Stable - Sources
baseurl={{ docker_ce_centos_yum_repository_url }}/$releasever/source/stable
enabled=0
gpgcheck=1
